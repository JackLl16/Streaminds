@model Streaminds.Web.Models.Dto.ProductoStreamingDto
<div class="mb-3">
 <label asp-for="Nombre" class="form-label"></label>
 <input asp-for="Nombre" class="form-control" />
 <span asp-validation-for="Nombre" class="text-danger"></span>
</div>
<div class="mb-3">
 <label asp-for="Descripcion" class="form-label"></label>
 <textarea asp-for="Descripcion" class="form-control"></textarea>
 <span asp-validation-for="Descripcion" class="text-danger"></span>
</div>
<div class="mb-3">
 <label asp-for="TipoProducto" class="form-label"></label>
 <input asp-for="TipoProducto" class="form-control" />
 <span asp-validation-for="TipoProducto" class="text-danger"></span>
</div>
<div class="mb-3 row">
 <div class="col-md-4">
 <label asp-for="Precio" class="form-label"></label>
 <input asp-for="Precio" class="form-control" />
 <span asp-validation-for="Precio" class="text-danger"></span>
 </div>
 <div class="col-md-4">
 <label asp-for="DuracionDias" class="form-label"></label>
 <input asp-for="DuracionDias" class="form-control" />
 <span asp-validation-for="DuracionDias" class="text-danger"></span>
 </div>
 <div class="col-md-4">
 <label asp-for="CalidadVideo" class="form-label"></label>
 <input asp-for="CalidadVideo" class="form-control" />
 <span asp-validation-for="CalidadVideo" class="text-danger"></span>
 </div>
</div>
<div class="form-check mb-3">
 <input asp-for="Activo" class="form-check-input" />
 <label asp-for="Activo" class="form-check-label"></label>
</div>
<input type="hidden" asp-for="Id" />